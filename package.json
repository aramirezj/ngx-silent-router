{
  "name": "@aramirezj/ngx-silent-router",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve -o",
    "build": "npm run build:library",
    "build:library": "ng build --project=ngx-silent-router && npm run copy-readme",
    "watch": "ng build --project=ngx-silent-router --watch",
    "serve:api": "json-server db.json",
    "bump-version": "rjp package.json version $VERSION",
    "copy-readme": "copyfiles README.md dist/ngx-silent-router",
    "copy-docs": "copyfiles dist/ngx-silent-router-showcase/ docs/",
    "customize": "node form/customization.mjs",
    "lint:library": "ng lint ngx-silent-router",
    "lint:playground": "ng lint ngx-silent-router-showcase",
    "commitlint": "commitlint --edit",
    "watch:library": "jest --config projects/ngx-silent-router/jest.config.js --watch",
    "test:library": "jest --config projects/ngx-silent-router/jest.config.js",
    "test:showcase": "jest --config projects/ngx-silent-router-showcase/jest.config.js",
    "coverage:library": "jest --config projects/ngx-silent-router/jest.config.js --coverage --coverageDirectory=../../.coverage",
    "format:test": "prettier --list-different 'projects/**/*.{ts,html}'",
    "format:write": "prettier --write 'projects/**/*.{ts,html}'",
    "prepare": "husky install",
    "stats": "ng build ngx-silent-router-showcase --stats-json && webpack-bundle-analyzer dist/ngx-silent-router-showcase/stats.json",
    "lint": "ng lint"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.2.0",
    "@angular/common": "^15.2.0",
    "@angular/compiler": "^15.2.0",
    "@angular/core": "^15.2.0",
    "@angular/forms": "^15.2.0",
    "@angular/material": "^15.2.0",
    "@angular/platform-browser": "^15.2.0",
    "@angular/platform-browser-dynamic": "^15.2.0",
    "@angular/router": "^15.2.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.12.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.2.9",
    "@angular/cli": "~15.2.9",
    "@angular/compiler-cli": "^15.2.0",
    "@types/jasmine": "~4.3.0",
    "jasmine-core": "~4.5.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "typescript": "~4.9.4",
    "@angular-eslint/builder": "15.2.0",
    "@angular-eslint/eslint-plugin": "15.2.0",
    "@angular-eslint/eslint-plugin-template": "15.2.0",
    "@angular-eslint/schematics": "15.2.0",
    "@angular-eslint/template-parser": "15.2.0",
    "@briebug/jest-schematic": "^2.1.1",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/exec": "^6.0.3",
    "@types/jest": "29.5.1",
    "@types/node": "^18.16.3",
    "@typescript-eslint/eslint-plugin": "5.59.2",
    "@typescript-eslint/parser": "5.59.2",
    "autoprefixer": "^10.4.14",
    "copyfiles": "^2.4.1",
    "eslint": "^8.39.0",
    "eslint-plugin-deprecation": "^1.4.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsdoc": "^43.1.1",
    "husky": "^8.0.3",
    "jest": "29.5.0",
    "ng-packagr": "^15.2.2",
    "ngx-deploy-npm": "^5.2.0",
    "ngx-highlightjs": "^8.0.0",
    "postcss": "^8.4.23",
    "prettier": "^2.8.8",
    "replace-json-property": "^1.8.0",
    "semantic-release": "^21.0.2"
  },
  "author": "Alejandro Ramírez Jiménez https://www.linkedin.com/in/aramirezjimenez/",
  "publishConfig": {
    "@aramirezj:registry": "https://npm.pkg.github.com"
  },
  "keywords": [
    "javascript",
    "typescript",
    "angular",
    "angular-router",
    "router",
    "angular16",
    "generic"
  ],
  "license": "MIT"
}
